fn main() {
    'outer: for i in 1..=10 {
        'inner: for j in 1..=10 {
            print!("{:4}", i * j);
            if j == 5 && i == 5 {
                println!("\nBreaking out of 'outer loop at i = {}", i);
                break 'outer;
            }
        }
        println!(); 
    }
}
